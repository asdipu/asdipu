int isZeroPlentiful(int[] a){
  int countZero =0;
  int zeroPlenti =0;
  for(int i=0;i<a.length;i++){
    if(a[i]==0){
      countZero++;
    }else{
      if(countZero>=4){
        zeroPlenti++;
      }else if(countZero<4){
         return 0;
      }
      countZero=0;
    }
  }
  if(countZero<4) return 0;
  else if(countZero>=4){
    zeroPlenti++;
  }
  return zeroPlenti;
}
