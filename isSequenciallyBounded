 public class Main{
  public static void main(String[] args){
    System.out.println(isSequenciallyBounded(new int[] ));
  }
  public static int isSequenciallyBounded(int[] a){
    if(a.length==0) return 1;
    int count = 1;
    for(int i=1;i<a.length;i++){
         if(a[i]==a[i-1]){
          count++;
            if(count>=a[i]) return 0;
         }else if(a[i]>a[i-1]){
          count = 1;
         }else return 0;
      }
      return 1;
  }
} 
